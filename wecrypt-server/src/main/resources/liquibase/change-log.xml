<?xml version="1.0" encoding="UTF-8"?>
<databaseChangeLog
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
         http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.1.xsd">

    <changeSet id="1" author="ambrose">
        <createTable tableName="users">
            <column name="id" type="integer" autoIncrement="true">
                <constraints primaryKey="true"/>
            </column>
            <column type="varchar(64)" name="username"/>
        </createTable>
    </changeSet>

    <changeSet id="2" author="ambrose">
        <sql>
            insert into users(username) values('ambrose')
            insert into users(username) values('lucy')
            insert into users(username) values('mary')
        </sql>
    </changeSet>

</databaseChangeLog>